pipeline {
  agent none
 
    stages {
    stage ( 'BUILD' ) {
            agent { label 'masterkeynode' }
                steps {
                 sh '''       
                    cd /var/lib/jenkins/workspace/pipeline_jenkins
                    make ABC.exe
                    '''
                    }
                 }
               stage ( 'TEST' ) {
                  agent { label 'node1-cproject' }
                steps {
                 sh '''
                    echo "$$"
                    '''
                    }
                 }
                  stage ( 'pipeline-test' ) {
                     agent { label 'masterkeynode' }
                steps {
                 sh '''
                    echo "$!"
                    '''
                    }
                 }
             
             stage ( 'pipeline-deploy' ) {
                agent { label 'node1-cproject' }
                steps {
                 sh '''
                    pwd
                    '''
                    }
                 }
                  stage ( 'pipeline-build' ) {
                     agent { label 'node1-cproject' }
                steps {
                 sh '''
                    echo "$?"
                    '''
                    }
                 }
            }
         }

